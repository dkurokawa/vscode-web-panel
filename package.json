{
  "name": "dash-panel",
  "displayName": "Dash Panel",
  "description": "Display web dashboards directly in VS Code panels or side views.",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dash-panel"
  },
  "homepage": "https://github.com/yourusername/dash-panel#readme",
  "bugs": {
    "url": "https://github.com/yourusername/dash-panel/issues"
  },
  "license": "MIT",
  "icon": "icon.png",
  "keywords": [
    "dashboard",
    "webview",
    "panel",
    "monitoring",
    "web"
  ],
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dash-panel.openDashboard",
        "title": "Open Dashboard Panel"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "dash-panel.dashboardView",
          "name": "Dash Panel"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
